	pstr__seq:size_type -> pstr__seq:str_size [color="#404040"];
	index__seq:files_type -> file__seq [style=bold];
	index__seq:file_count_type -> index__seq:files__repeat [color="#404040"];
	file__seq:name_type -> pstr__seq [style=bold];
